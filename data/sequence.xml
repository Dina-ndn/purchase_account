<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data >
        <!-- expense start-->
        <record id="seq_wnz_pur_exp_vr_code" model="ir.sequence">
            <field name="name">Expense Sequence</field>
            <field name="code">waaneiza.pur.settlement.vr</field>
            <field name="prefix">SE-</field>
            <field name="padding">5</field>
            <field name="number_next">1</field> 
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_wnz_pur_exp_info_code" model="ir.sequence">
            <field name="name">Expense Sequence</field>
            <field name="code">waaneiza.pur.settlement.info</field>
            <field name="prefix">%(y)s/EXP</field>
            <field name="padding">5</field>
            <field name="number_next">1</field> 
            <field name="number_increment">1</field> 
            <field name="company_id" eval="False"/>
        </record>

        <!-- expense close-->
    </data> 
</odoo>